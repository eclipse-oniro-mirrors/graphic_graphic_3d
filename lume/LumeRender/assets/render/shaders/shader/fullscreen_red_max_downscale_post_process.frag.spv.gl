#version 450

uniform sampler2D s1_b1_s1_b0;

layout(location = 0) in vec2 inUv;
layout(location = 0) out float outColor;

void main()
{
    outColor = max(max(texture(s1_b1_s1_b0, inUv).x, textureOffset(s1_b1_s1_b0, inUv, ivec2(1, 0)).x), max(textureOffset(s1_b1_s1_b0, inUv, ivec2(0, 1)).x, textureOffset(s1_b1_s1_b0, inUv, ivec2(1)).x));
}

